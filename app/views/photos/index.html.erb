<div class="container">
  <div class="box">
    <div class="row">
      <% @photos.each do |photo|%>
        <div class="col-xs-12 col-sm-4">
          <%= link_to photo_path(photo), class: "card-link" do %>
            <div class="card-company">
              <%= image_tag(url_for(photo.image)) %>
            </div>
          <% end %>
        </div>
      <% end %>
    </div>
  </div>
</div>
